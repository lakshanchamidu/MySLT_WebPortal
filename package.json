{
  "name": "slt-web-portal",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vite",
    "deploy": "gh-pages -d dist",
    "build": "node --max-old-space-size=4096 node_modules/vite/bin/vite.js build",
    "lint": "eslint .",
    "analyze": "cross-env NODE_OPTIONS=--max-old-space-size=8192 vite build --mode production && source-map-explorer dist/assets/**/*.js",
    "preview": "set NODE_OPTIONS=--max-old-space-size=4096 && vite preview",
    "build:prod": "set NODE_OPTIONS=--max-old-space-size=8192 && vite build --mode production"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/inter": "^5.1.0",
    "@fontsource/poppins": "^5.1.0",
    "@fontsource/sarabun": "^5.1.0",
    "@greatsumini/react-facebook-login": "^3.4.0",
    "@mui/icons-material": "^6.1.4",
    "@mui/material": "^6.4.12",
    "@mui/x-charts": "^7.21.0",
    "@mui/x-date-pickers": "^7.22.3",
    "@react-oauth/google": "^0.12.2",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.9.2",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.1.0",
    "jwt-decode": "^4.0.0",
    "mui-one-time-password-input": "^3.0.1",
    "react": "^18.3.1",
    "react-datepicker": "^8.2.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.5.2",
    "react-icons": "^5.5.0",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.27.0",
    "recharts": "^2.15.3",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "gh-pages": "^6.3.0",
    "globals": "^15.9.0",
    "source-map-explorer": "^2.5.3",
    "terser": "^5.42.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8"
  }
}
